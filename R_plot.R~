data <- read.csv("/Users/lesser/git/thesis/processing/prot_csv/test/test_0.csv")
colnames(data) <- c("TIME","DISTANCE","PRESSURE")
plot(data$TIME,data$DISTANCE,type="l",col="red",lwd=1,axes=FALSE,xlab="TIME",ylab="DISTSNCE")
axis(1)
axis(2)
par(new=TRUE)
plot(data$TIME,data$PRESSURE,type="l",col="red",lwd=1,axes=FALSE,xlab="",ylab="")
mtext("PRESSURE",side=4,line=3)
axis(4)
box()